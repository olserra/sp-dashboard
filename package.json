{
  "name": "skills-and-pills",
  "description": "Skills & Pills Dashboard",
  "version": "1.0.0",
  "private": true,
  "author": "Otavio Serra",
  "type": "commonjs",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "tailwind",
    "theme",
    "starter",
    "gpt4",
    "typescript",
    "nextjs",
    "law",
    "legal",
    "pdf"
  ],
  "scripts": {
    "dev": "next",
    "build": "npx prisma generate next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "npx prisma generate && next build"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn add lint --fix",
      "yarn add format"
    ]
  },
  "dependencies": {
    "@types/formidable": "^3.4.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "axios": "^1.5.1",
    "chart.js": "^4.4.0",
    "classnames": "^2.3.2",
    "next": "^13.4.10",
    "next-auth": "^4.22.3",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "next-seo": "^4.29.0",
    "node-fetch": "^3.3.1",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@roketid/windmill-react-ui": "^0.1.1",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "20.8.7",
    "@types/react": "18.2.29",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.6",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
